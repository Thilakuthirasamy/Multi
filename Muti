import java.io.*;
import java.util.*;
public class Multi
{
    public static void main(String args[])
    {
      Scanner s=new Scanner(System.in);
      Double n1=Double.parseDouble(s.nextLine());
      Double n2=Double.parseDouble(s.nextLine());
      Double n3=n1*n2;
      System.out.print(n3);
      
    }
}
